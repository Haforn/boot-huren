
<div class="col-2 col-right">
	<div class="col-4 col-right">	
		<% if @current_user %>
			<%= link_to "My profile", user_path(@current_user.id), :class => 'btn btn-default' %> 
		<% else %>
			<%= link_to "Sign up", new_user_path, :class => 'btn btn-default' %>
		<% end %>
	</div>
	<div class="col-4 col-right">
		<% if !@current_user %> 
			<%= link_to "Login", login_path, :class => 'btn btn-default' %>
		<% else %>
			<%= link_to("Log out", {:controller => 'access', :action => 'logout'}, :class => 'btn btn-default') %>
		<% end %>
	</div>
</div>